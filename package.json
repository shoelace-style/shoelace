    "generate": "stencil generate"
  },
  "devDependencies": {
    "@stencil/core": "^1.8.3",
    "@stencil/sass": "^1.1.1",
    "tslint": "^5.20.1",
    "tslint-ionic-rules": "0.0.21",
    "tslint-stencil": "^1.0.1",
    "workbox-build": "4.3.1"
  },
  "license": "MIT",
  "dependencies": {}
